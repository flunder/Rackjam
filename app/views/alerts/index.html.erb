<div id="staticResults" class="<%=controller.controller_name %>">
	<h3>My alerts</h3>
	<h4></h4>

	<table>
	  <tr>
	    <th></th>
	    <th></th>
	    <th></th>
	    <th></th>			
	    <th></th>
	    <th></th>
	    <th></th>
	  </tr>

	<% @alerts.each do |alert| %>
	  <tr>
	    <td><%= alert.freetext %></td>
	    <td><%= alert.price %></td>		
	    <td><%= alert.site %></td>			
	    <td><%= alert.user_id %></td>				
	    <td><%= link_to 'Show', alert %></td>
	    <td><%= link_to 'Edit', edit_alert_path(alert) %></td>
	    <td><%= link_to 'Destroy', alert, confirm: 'Are you sure?', method: :delete %></td>
	  </tr>
	<% end %>
	</table>

	<br />

	<%= link_to 'New Alert', new_alert_path %>
</div>