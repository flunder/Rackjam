<!DOCTYPE html>
<html>

<head>
  <title>Synth4</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults, 'isotope/jquery.isotope.min.js' %>
  <%= auto_discovery_link_tag :atom, "/feed" %>
  <%= auto_discovery_link_tag :rss, "/feed.rss" %>
  <%= csrf_meta_tag %>
</head>

<body>
	<div id="header">
		<%= render "layouts/header" %>
	</div>
	
	<div id="main">	
		<div id="leftNav">
			<%= render "layouts/leftnav" %>
		</div>
    	<%= yield  %>
	</div>	
	
	<%= render "layouts/tracking" %>
</body>
</html>

